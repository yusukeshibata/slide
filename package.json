{
  "name": "slide",
  "version": "1.0.0",
  "description": "Slideshow generator",
  "main": "karma.conf.js",
  "scripts": {
    "updates": "ncu -ua && npm install",
    "jade": "NODE_ENV=development jade src --obj config/development.json --out development",
    "jade:production": "NODE_ENV=production jade src --obj config/production.json --out production",
    "build:production": "NODE_ENV=production webpack && npm run jade:production",
    "build": "NODE_ENV=development webpack && npm run jade",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "start": "NODE_ENV=development node server.js",
    "test": "karma start",
    "build:test": "node bin/build.js -o src/datas/test.json ~/Desktop/test.md && npm run jade",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yusukeshibata/slide.git"
  },
  "keywords": [
    "slide",
    "generator",
    "react"
  ],
  "author": "Yusuke Shibata<shibata@fata.io>",
  "devDependencies": {
    "appcache-webpack-plugin": "^1.3.0",
